config BOARD
    default "nucleo-h723zg" if BOARD_NUCLEO_H723ZG

config BOARD_NUCLEO_H723ZG
    bool
    default y
    select BOARD_COMMON_NUCLEO144
    select CPU_MODEL_STM32H723ZG

source "$(RIOTBOARD)/common/nucleo144/Kconfig"
